{
  "expo": {
    "name": "above-and-below-encounters",
    "slug": "above-and-below-encounters",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/aab-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/aab-splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#369fd5"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    }
  }
}
